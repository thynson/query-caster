{
  "name": "query-caster",
  "version": "0.0.1-alpha1",
  "description": "Yet another query builder written in Typescript",
  "main": "built/index.js",
  "typings": "built/index.d.ts",
  "scripts": {
    "prepublish": "typings install && tsc",
    "test": "node -r ts-node/register test/test.ts | tap-summary",
    "test:coverage": "nyc -e .ts -i ts-node/register node test/test.ts",
    "coveralls": "nyc report -r text-lcov | coveralls"
  },
  "keywords": [
    "SQL",
    "builder",
    "Typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/thynson/query-caster.git"
  },
  "author": "LAN Xingcan",
  "license": "ISC",
  "devDependencies": {
    "coveralls": "^2.11.12",
    "nyc": "^8.1.0",
    "tap-summary": "^3.0.1",
    "tape": "^4.6.0",
    "ts-node": "^1.3.0",
    "typescript": "^2.0.0",
    "typings": "^1.3.2"
  }
}
